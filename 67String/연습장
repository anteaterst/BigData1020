class Main {
  public static void main(String[] args) {

    String text = "Hellow Java";
    String com = "Hellow Java";

    
    System.out.println(text.charAt(2)); // 몇 번째
    System.out.println(text.concat("s")); // 붙인다. (연결)
    System.out.println(text.contains("And"));
    System.out.println(text.contains("Java")); //포함하고있는지 판별 
    System.out.println(text.equals(com)); // 같은지 비교 판단
    System.out.println(text.equalsIgnoreCase(com));
    System.out.println(text.indexOf('a')); // 처음등장하는 글자의 위치 값 반환
    System.out.println(text.indexOf('b')); // 찾는 글자가 없으면 -1 반환
    System.out.println(text.lastIndexOf('a')); // 처음 등장하는 글자의 위치 값 반환
  
    System.out.println(text.trim()); //공백 끝에 공백 여백 제거

    String temp = text.trim();
    
    System.out.println(text.length()); // 글자의 개수 반환 

    System.out.println(text.substring(7)); // 7부터 짜르기 

    System.out.println(text.substring(7,9)); //7부터 9 미만까지 짜르기 
    System.out.println(text.replace(" ","-")); // 모두 대문자로
    System.out.println(text.toLowercase()); //모두 소문자로


    String[] sp = text.split(" ");
    for(int i = 0 ; i < sp.length ; i ++) {
      System.out.println(sp[i]);
    }




  }
}
